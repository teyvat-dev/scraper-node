{
  "name": "teyvatdev-scraper-node",
  "version": "0.3.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "functions-framework",
    "dev": "concurrently \"tsc -w\" \"nodemon --watch ./build/ --exec npm run start\"",
    "deploy": "yarn build && gcloud functions deploy $FUNCTION_TARGET --runtime nodejs12 --trigger-http --allow-unauthenticated",
    "build": "yarn run clean && yarn run compile && cp package.json yarn.lock ./build/src",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile",
    "posttest": "yarn run lint"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^1.7.1",
    "cheerio": "^1.0.0-rc.3",
    "node-fetch": "^2.6.1",
    "tiny-async-pool": "^1.2.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.22",
    "@types/express": "^4.17.9",
    "@types/node": "^14.11.2",
    "@types/node-fetch": "^2.5.7",
    "@types/tiny-async-pool": "^1.0.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.14.0",
    "gts": "^3.0.2",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "typescript": "^4.0.3"
  }
}
